{{#paper-dialog onClose=(action 'closeDialog') origin=origin clickOutsideToClose=true}}
  {{#paper-toolbar}}
    <div class="md-toolbar-tools">
      <h2>Perceel detail</h2>
      <span class="flex"></span>
      {{#paper-button iconButton=true onClick=(action 'closeDialog')}}{{paper-icon icon="close"}}{{/paper-button}}
    </div>
  {{/paper-toolbar}}

  {{#paper-dialog-content}}
    <img src="assets/perceel doorklik.png" />
  {{/paper-dialog-content}}

  {{#paper-dialog-actions class="layout-row"}}
    <span class="flex"></span>
    {{#paper-button onClick=(action 'closeDialog')}}Close{{/paper-button}}
  {{/paper-dialog-actions}}
{{/paper-dialog}}
